@{
    ViewData["Title"] = "Perfiles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-12">
        <div class="card border-0">
            <div class="col-md-12 px-2 pt-2">
                <ul class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">PQRSF</a></li>
                    <li class="breadcrumb-item active">Perfiles</li>
                </ul>
            </div>
            <div class="card-body">
                <ul class="nav nav-tabs nav-tabs-v2">
                    <li class="nav-item me-3"><a href="#Etiquetas" class="nav-link active" data-bs-toggle="tab">Etiquetas</a></li>
                    <li class="nav-item me-3"><a href="#Preguntas" class="nav-link" data-bs-toggle="tab">Preguntas</a></li>                   
                    <li class="nav-item me-3"><a href="#Areas" class="nav-link" data-bs-toggle="tab">Áreas</a></li>
                    <li class="nav-item me-3"><a href="#Agentes" class="nav-link" data-bs-toggle="tab">Agentes</a></li>
                </ul>

                <div class="tab-content pt-3">
                    <div class="tab-pane fade active show" id="Etiquetas">
                        <partial name="Gets/GetEtiquetas" />
                    </div>

                    <div class="tab-pane fade" id="Preguntas">
                        <partial name="Gets/GetPreguntas" />
                    </div>

                    <div class="tab-pane fade" id="Areas">
                        <partial name="Gets/GetAreas" />
                    </div>
                    <div class="tab-pane fade" id="Agentes">
                        <partial name="Gets/GetAgentes" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts
    {
    <script src="~/scripts/Profiler/profiler.js" type="text/javascript"></script>
    <script>
        //ShowMessage("@TempData["Message"]");
    </script>
}